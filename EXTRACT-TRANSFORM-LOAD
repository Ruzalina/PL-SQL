EXTRACT-TRANSFORM-LOAD
select*
from hr.employees;

create table hr.employees_plsql_1
(empid number,
first_name varchar2(25),
last_name varchar2(25));

select EMPLOYEE_ID,FIRST_NAME,LAST_NAME
from hr.employees
where EMPLOYEE_ID=100;

DECLARE
V_EMPLOYEE_ID HR.EMPLOYEES.EMPLOYEE_ID%TYPE;
V_FIRST_NAME HR.EMPLOYEES.FIRST_NAME%TYPE;
V_LAST_NAME HR.EMPLOYEES.LAST_NAME%TYPE;
BEGIN
select EMPLOYEE_ID,FIRST_NAME,LAST_NAME INTO V_EMPLOYEE_ID,V_FIRST_NAME,V_LAST_NAME
from hr.employees
where EMPLOYEE_ID=100;
INSERT INTO hr.employees_plsql_1 VALUES(V_EMPLOYEE_ID,V_FIRST_NAME,V_LAST_NAME);
COMMIT;
END;

SELECT*
FROM hr.employees_plsql_1;

select*
from hr.employees;


BRINGING MORE THAN ONE ROW.....

create table hr.employees_plsql_2
(empid number,
first_name varchar2(25),
last_name varchar2(25));

DECLARE
V_EMPLOYEE_ID HR.EMPLOYEES.EMPLOYEE_ID%TYPE;
V_FIRST_NAME HR.EMPLOYEES.FIRST_NAME%TYPE;
V_LAST_NAME HR.EMPLOYEES.LAST_NAME%TYPE;
CURSOR C IS SELECT EMPLOYEE_ID,FIRST_NAME,LAST_NAME
FROM HR.EMPLOYEES;
BEGIN
OPEN C;
LOOP
FETCH C INTO V_EMPLOYEE_ID,V_FIRST_NAME,V_LAST_NAME;
EXIT WHEN C % NOTFOUND;
INSERT INTO  hr.employees_plsql_2 VALUES(V_EMPLOYEE_ID,V_FIRST_NAME,V_LAST_NAME);
COMMIT;
END LOOP;
CLOSE C;
END;
/

SELECT*
FROM hr.employees_plsql_2;


